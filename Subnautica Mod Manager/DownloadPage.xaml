<Window x:Class="SubnauticaModManager.Wpf.DownloadPage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:mui="http://schemas.modernwpf.com/2019"
        xmlns:local="clr-namespace:SubnauticaModManager"
        mc:Ignorable="d"
        mui:WindowHelper.UseModernWindowStyle="True"
        mui:ThemeManager.IsThemeAware="True"        
        Background="{DynamicResource SystemControlPageBackgroundAltHighBrush}"
        Foreground="{DynamicResource SystemControlPageTextBaseHighBrush}"
        Title="DownloadPage" Height="600" Width="800">
    <ScrollViewer>
        <StackPanel>
            <!--<Image Source="{Binding mod.OnlineInfo.picture_url}" Width="300"/>-->
            <!--<WebBrowser Name="DescFrame" Height="400"/>-->
            <Button Content="Download" Name="DlBtn"/>
            <Button Content="I have downloaded" Name="DledBtn"/>
        </StackPanel>
    </ScrollViewer>
</Window>
